{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build SuperMarioOdysseyOnline.Server",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/SuperMarioOdysseyOnline.Server/SuperMarioOdysseyOnline.Server.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary;ForceNoAlign"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "publish SuperMarioOdysseyOnline.Server",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
                "${workspaceFolder}/SuperMarioOdysseyOnline.Server/SuperMarioOdysseyOnline.Server.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary;ForceNoAlign"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "watch SuperMarioOdysseyOnline.Server",
            "command": "dotnet",
            "type": "process",
            "args": [
                "watch",
                "run",
                "--project",
                "${workspaceFolder}/SuperMarioOdysseyOnline.Server/SuperMarioOdysseyOnline.Server.csproj",
            ],
            "problemMatcher": "$msCompile"
        }
    ]
}
